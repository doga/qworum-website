# -- Document metadata --------------------------------------------------
<>
  a foaf:PersonalProfileDocument;
  dct:description "A person's profile"@en;
  foaf:maker <#id>;
  foaf:primaryTopic <#id>;
  cc:license <https://creativecommons.org/licenses/by-nc/4.0/>.
  # cc:license <https://creativecommons.org/publicdomain/zero/1.0/>.


# -- Person --------------------------------------------------
<#id>
  a foaf:Person, schema:Person, prov:Agent, cv:Person;
  foaf:name 'Doğa Armangil';
  foaf:givenname 'Doğa';
  foaf:family_name 'Armangil';
  foaf:mbox <mailto:d.armangil@qworum.net>;
  foaf:img <#portrait>, <#ascii-art>;
  cv:gender 'Male';
  cv:hasCitizenship country:TR, country:GB;
  cv:hasNationality 'Turkish'@en, 'British'@en;
  rdfs:isDefinedBy 
    <https://www.linkedin.com/in/dogaarmangil/>, 
    <https://twitter.com/DogaArmangil>.

<#cv>
  a cv:CV;
  cv:aboutPerson <#id>;
  cv:hasSkill <#english>, <#french>, <#turkish>, <#german>;
  cv:hasEducation <#master>.

<#master>
  a cv:Education;
  cv:studiedIn <#epfl>;
  cv:degreeType "Master's degree";
  cv:eduMajor 'Software engineer'@en;
  cv:eduMinor 'Software applications'@en.

<#epfl>
  a cv:EducationalOrg;
  rdfs:isDefinedBy <https://epfl.ch>.

<#english>
  a cv:LanguageSkill;
  cv:skillName 'English'@en;
  cv:skillLevel 5.

<#french>
  a cv:LanguageSkill;
  cv:skillName 'French'@en, 'Français'@fr;
  cv:skillLevel 5.

<#turkish>
  a cv:LanguageSkill;
  cv:skillName 'Turkish'@en, 'Türkçe'@tr;
  cv:skillLevel 5.

<#german>
  a cv:LanguageSkill;
  cv:skillName 'German'@en, 'Deutsch'@de;
  cv:skillLevel 5.

<#patent>
  a bibo:Patent;
  dct:creator <#id>;
  dct:title 'Method and a system for the composition of services'@en;
  bibo:uri <https://patents.google.com/patent/US8266632>.

<#portrait>
  a schema:ImageObject, foaf:Image;
  schema:contentUrl <https://qworum.net/assets/images/portraits/D-Armangil-300x300.jpg>;
  schema:encodingFormat 'image/jpeg';
  prov:wasAttributedTo <#id>.

<#ascii-art>
  a schema:ImageObject, foaf:Image;
  schema:contentUrl <https://qworum.net/assets/images/portraits/DoğaArmangil.ascii>;
  schema:encodingFormat 
    'text/vnd.ascii-art', 
    <https://www.iana.org/assignments/media-types/text/vnd.ascii-art>;
  prov:wasGeneratedBy [
    a prov:Activity;
    dct:description 'The ASCII Art Archive provides an online image conversion service, which was used here.'@en;
    prov:used <#portrait>;
    prov:wasAssociatedWith <#id>, <org.ttl#ascii-art-archive>
  ];
  prov:wasDerivedFrom <#portrait>;
  prov:wasAttributedTo <#id>.

